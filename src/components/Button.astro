---
// export interface
interface Props{
    text:string
}
const {text } =Astro.props;
---

<button>{text}</button>

<style is:inline>
    button{
        background: hsl(176, 50%, 47%);
        border-radius: 30px;
        padding: 1em 2.5em;
        color: white;
        font-family:'bold';
    }
</style>